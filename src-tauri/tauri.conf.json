{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "TutorMate",
  "version": "0.1.6",
  "identifier": "com.student-management.app",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "수강생 관리 프로그램",
        "width": 1400,
        "height": 900,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "publisher": "Student Management Team"
  },
  "plugins": {
    "updater": {
      "endpoints": [
        "https://github.com/rlawlghkd12/tutomate/releases/latest/download/latest.json"
      ],
